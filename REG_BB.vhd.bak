LIBRARY IEEE;
USE IEEE.STD_LOGIC_1164.ALL;

ENTITY REG_BB IS
PORT(
     OVER7:IN STD_LOGIC;
	  Y:IN STD_LOGIC_VECTOR(10 DOWNTO 0);
	  Q:BUFFER STD_LOGIC_VECTOR(10 DOWNTO 0));
END ENTITY;
ARCHITECTURE ART OF REG_BB IS
BEGIN
PROCESS(Y)IS
BEGIN

 IF (OVER6='0') THEN
 Q<="00000000000";
 ELSE Q<=Y;
 END IF;
 END PROCESS;
 Q<=Q;
 END ARCHITECTURE ART;